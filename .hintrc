{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "animation-timeline",
          "animation-timeline: view()"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off",
    "disown-opener": "off"
  }
}